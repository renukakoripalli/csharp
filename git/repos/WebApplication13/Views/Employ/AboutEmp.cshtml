@model IEnumerable<WebApplication6.model.Employ>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <h1>Employ Details</h1>
    <table border="1">
        <tr>
            <td>Id</td>
            <td>Name</td>
            <td>Email</td>
            <td>Dept</td>
        </tr>
        @foreach (var emp1 in Model)
        {
            <tr>
                <td>@emp1.Id</td>
                <td>@emp1.Name</td>
                <td>@emp1.Email</td>
                <td>@emp1.dept</td>
            </tr>
        }

    </table>
</body>
</html>
